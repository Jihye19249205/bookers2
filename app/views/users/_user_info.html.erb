<h2>User info</h2>
<p><%= image_tag user.get_profile_image, size:"100x100" %></p>
<table class = "table">
  <tbody>
    <tr>
      <th>name</th>
      <th><%= user.name %></th>
    </tr>
    <tr>
      <th>introduction</th>
      <th><%= user.introduction %></th>
    </tr>
  </tbody>
</table>
<button class>
  <%= link_to "Update", edit_user_path(user.id) %>
</button>
